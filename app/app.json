{
  "expo": {
    "name": "MANHUNT",
    "slug": "manhunt-tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "backgroundColor": "#000000",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.manhunt.tracker"
    },
    "android": {
      "package": "com.manhunt.tracker",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyD7HPw0wtxb8tmrRI33CdvLUWERUZuoW_4"
        }
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "CAMERA",
        "INTERNET",
        "FOREGROUND_SERVICE",
        "FOREGROUND_SERVICE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow MANHUNT to use your location.",
          "locationAlwaysPermission": "Allow MANHUNT to use your location in the background.",
          "locationWhenInUsePermission": "Allow MANHUNT to use your location while you are using the app."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "manhunt-tracker"
      }
    }
  }
}
